{
  "name": "Pokemon",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "start:dev": "cross-env BASE_API_URL=https://pokeapi.co/api/v2 react-native start",
    "android:dev": "cross-env BASE_API_URL=https://pokeapi.co/api/v2 react-native run-android",
    "ios:dev": "cross-env BASE_API_URL=https://pokeapi.co/api/v2 react-native run-ios",
    "test": "jest"
  },
 "jest": {
  "preset": "react-native",
  "setupFilesAfterEnv": ["<rootDir>/src/jest.setup.ts"],
  "transformIgnorePatterns": [
    "node_modules/(?!(@react-native|react-native|@react-navigation|@shopify/flash-list|lottie-react-native|react-native-sound|@reduxjs/toolkit|immer)/)"
  ],
  "setupFiles": ["<rootDir>/node_modules/react-native-gesture-handler/jestSetup.js"],
  "moduleNameMapper": {
    "^@env$": "<rootDir>/__mocks__/@env.js"
  }
},
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^9.5.0",
    "@react-native/new-app-screen": "0.82.0",
    "@react-navigation/native": "^7.1.18",
    "@react-navigation/native-stack": "^7.3.28",
    "@reduxjs/toolkit": "^2.9.0",
    "@shopify/flash-list": "^2.1.0",
    "@types/react-native-dotenv": "^0.2.2",
    "install": "^0.13.0",
    "lottie-react-native": "^7.3.4",
    "react": "19.1.1",
    "react-native": "0.82.0",
    "react-native-dotenv": "^2.5.3",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-reanimated": "^4.1.3",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-sound": "^0.13.0",
    "react-native-svg": "^15.14.0",
    "react-native-svg-transformer": "^1.5.1",
    "react-native-worklets": "^0.6.1",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.82.0",
    "@react-native/eslint-config": "0.82.0",
    "@react-native/metro-config": "0.82.0",
    "@react-native/typescript-config": "0.82.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.1",
    "@types/react-test-renderer": "^19.1.0",
    "cross-env": "^10.1.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "msw": "^2.11.5",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.1",
    "typescript": "^5.8.3",
    "whatwg-fetch": "^3.6.20"
  },
  "engines": {
    "node": ">=20"
  }
